<h1>Professionels#new</h1>
<p>Find me in app/views/professionels/new.html.erb</p>



<%= simple_form_for(@professionel) do |p| %>
  <%= p.label :name %>
  <%= p.text_field :name %>
  <%= p.label :adress %>
  <%= p.text_field :adress %>
  <%= p.label :category %>
  <%= p.text_field :category %>
  <%= p.label :tarif %>
  <%= p.text_field :tarif %>
